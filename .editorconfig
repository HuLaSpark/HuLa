# 编辑器配置, @see: http://editorconfig.org

root = true

[*]
charset = utf-8
indent_size = 2
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
# 对于自动生成的文件，保持原有格式
[src-tauri/gen/schemas/**]
insert_final_newline = false
trim_trailing_whitespace = false

[tauri-plugin-hula/permissions/schemas/**]
insert_final_newline = false
trim_trailing_whitespace = false