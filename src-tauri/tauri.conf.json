{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "HuLa",
  "version": "3.0.7",
  "identifier": "com.hula.pc",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://127.0.0.1:6130"
  },
  "bundle": {
    "category": "SocialNetworking",
    "copyright": "HuLaSpark",
    "licenseFile": "./copyright/License.rtf",
    "resources": [
      "tray",
      "configuration",
      "draco",
      "docs"
